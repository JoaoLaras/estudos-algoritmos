Algoritmo "semnome"

Var
   contador: inteiro


Inicio
      Escreval("Inicio: 8 ")
      Escreval("Fim: 0 ")
      Escreval("-----------------")
      Escreval("    CONTANDO     ")
      Escreval("-----------------")
      contador <- 8
      Enquanto (contador >= 0) faca
        Escreva(contador)
        contador <- (contador - 1)
      Fimenquanto



Fimalgoritmo