algoritmo "semnome"
var
contador, N, S : inteiro
inicio
      contador <- 1
      S <- 0
      Enquanto (contador <= 10) faca
               Escreval("Digite um numero:", contador, "o. valor:" )
               Leia(N)
               S <- S + N
                Contador <- contador + 1
      FIMENQUANTO
      Escreval(" A soma dos numero e ", S )


fimalgoritmo